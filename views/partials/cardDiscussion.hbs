{{#each discussion}}
    <div class="cardDiscussion">
        <img class="profilePic" src="https://randomuser.me/api/portraits/med/women/17.jpg" />
        <div class="cardDiscussionText">
            <div class="cardDiscussionHeader">
                <span class="subjectDiscussion">Dummy Topic Subject</span>
                <button class="btnInverse disabled">{{this.tag}}</button>
            </div>
            <p>{{this.details}}</p>
            <div class="cardDiscussionFooter">
                <p class="dateDiscussion">Jan 01 2020</p>
                <p ><a href="/">x replies</a></p>
            </div>
        </div>
    </div>
{{/each}}